{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Tracing.Worker": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "TracingSample": {
    "Akka": {
      "HoconPath": "akka.hocon"
    },
    "ExternalApi": {
      "BaseUrl": "http://localhost:5146"
    },
    "Redis": {
      "ConnectionString": "localhost:6379,abortConnect=false"
    },
    "WorkflowMonitor": {
      "Enabled": true,
      "InitialDelaySeconds": 5,
      "IntervalSeconds": 10,
      "TimeoutMinutes": 10
    }
  }
}